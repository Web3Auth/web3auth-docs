{
  "expo": {
    "name": "web3auth-react-native-example",
    "slug": "web3auth-react-native-example",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    // HIGHLIGHTSTART-platformSetup
    "scheme": "w3akvrn",
    // HIGHLIGHTEND-platformSetup
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/splash.png",
